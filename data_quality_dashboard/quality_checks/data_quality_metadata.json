{
    "file_name": "lightly_active_minutes.json",
    "expected_records": 152,
    "expected_variables": {
      "participant": {
        "type": "ForeignKey",
        "related_model": "ParticipantOverview",
        "description": "Link to the participant's overview."
      },
      "dateTime": {
        "type": "DateTimeField",
        "description": "Date and time of the recorded data.",
        "expected_format": "YYYY-MM-DD HH:MM:SS",
        "allowed_range": {
          "start": "2019-11-01 00:00:00",
          "end": "2019-03-31 00:00:00"
        }
      },
      "value": {
        "type": "IntegerField",
        "description": "The number of lightly active minutes recorded.",
        "allowed_range": {
          "min": 0,
          "max": 1440
        }
      }
    },
    "constraints": {
      "uniform_time_format": true
    }
  }

